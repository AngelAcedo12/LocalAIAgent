{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "localaiagent",
  "version": "0.1.0",
  "identifier": "com.angel.localaiagent",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
    
  },
  "app": {
    "windows": [
      {
        "title": "localaiagent",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "sql": {
      "preload": [
        "sqlite:chat_local.sqlite"
      ]
    }
    
  },
  
  "bundle": {
    
    "resources": [],
    "externalBin": [
      "bin/llama-server"
    ],
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}